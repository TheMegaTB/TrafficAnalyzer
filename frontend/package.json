{
    "name": "TrafficAnalyzer-frontend",
    "version": "0.0.1",
    "description": "Standard ES6 Boilerplate configured with babel, webpack and eslint.",
    "main": "index.js",
    "scripts": {
        "start": "npm-run-all --parallel dev:server lint:watch",
        "dev:server": "webpack-dev-server --hot --inline",
        "watch": "webpack -w -d",
        "build": "webpack",
        "lint": "node_modules/.bin/esw webpack.config.* src --color",
        "lint:watch": "npm run lint -- --watch"
    },
    "keywords": [
        "es6"
    ],
    "author": "tilb",
    "license": "ISC",
    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "css-loader": "^0.28.7",
        "eslint": "^4.8.0",
        "eslint-watch": "^3.1.2",
        "file-loader": "^1.1.5",
        "image-webpack-loader": "^3.4.2",
        "npm-run-all": "^4.0.2",
        "style-loader": "^0.19.0",
        "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v3.1.3",
        "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
        "webpack": "^3.7.1",
        "webpack-dev-server": "^2.4.2"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "babel-preset-react": "^6.24.1",
